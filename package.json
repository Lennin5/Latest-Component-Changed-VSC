{
  "name": "latest-component-changed-vsc",
  "displayName": "Latest Component Changed",
  "description": "This extension prints the lastest component changed in the status bar of the code editor.",
  "publisher": "Lennin Lemus",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lennin5/Latest-Component-Changed-VSC"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Education",
    "Formatters",
    "Keymaps",
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "latest-component-changed.showSelector",
        "title": "Show Component Selector",
        "category": "Latest Component Changed"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  }
}
